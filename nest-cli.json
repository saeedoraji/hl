{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-test/src",
  "monorepo": true,
  "root": "apps/api-test",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api-test/tsconfig.app.json"
  },
  "projects": {
    "api-test": {
      "type": "application",
      "root": "apps/api-test",
      "entryFile": "main",
      "sourceRoot": "apps/api-test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-test/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "apartment": {
      "type": "application",
      "root": "apps/apartment",
      "entryFile": "main",
      "sourceRoot": "apps/apartment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apartment/tsconfig.app.json"
      }
    }
  }
}